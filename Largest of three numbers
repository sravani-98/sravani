/*Write a program that takes three numbers a,b, and c as input and prints the largest number amongst them.*/

#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int n1, n2, n3;
    cin>>n1>>n2>>n3;
    if(n1>=n2 && n1>=n3)
        cout<<n1;
    else if(n2>=n3 && n2>=n3)
        cout<<n2;
    else if(n3>=n1 && n3>=n2)
        cout<<n3;
    return 0;
}
